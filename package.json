{
  "name": "media-overlay-library",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com/AxisCommunications/media-overlay-library-js.git"
  },
  "version": "4.0.5",
  "description": "React library for SVG drawing.",
  "license": "MIT",
  "main": "dist/esm/index.js",
  "browser": "dist/esm/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/index.d.ts",
  "keywords": [
    "react",
    "svg"
  ],
  "scripts": {
    "lint": "yarn eslint && yarn prettier:check",
    "eslint": "eslint --ext '.js,.jsx,.ts,.tsx' lib/",
    "prettier:check": "prettier --check 'lib/**/*.{js,ts,jsx,tsx}'",
    "prettier:fix": "prettier --write 'lib/**/*.{js,ts,jsx,tsx}' --fix --loglevel silent",
    "build": "yarn build:esm && yarn build:bundle",
    "build:esm": "tsc",
    "build:bundle": "webpack --config webpack.config.js",
    "dev": "webpack-dev-server --config webpack.example.js --hot",
    "release": "sbin/release.sh"
  },
  "peerDependencies": {
    "pepjs": ">= 0.4.3 < 1",
    "react": ">= 16.8.2 < 17",
    "react-dom": ">= 16.8.2 < 17"
  },
  "dependencies": {
    "@juggle/resize-observer": "3.3.0"
  },
  "devDependencies": {
    "@babel/cli": "7.13.10",
    "@babel/core": "7.13.10",
    "@babel/plugin-proposal-class-properties": "7.13.0",
    "@babel/plugin-proposal-object-rest-spread": "7.13.8",
    "@babel/polyfill": "7.12.1",
    "@babel/preset-env": "7.13.10",
    "@babel/preset-react": "7.12.13",
    "@babel/preset-typescript": "7.13.0",
    "@types/react": "17.0.3",
    "@types/react-dom": "17.0.2",
    "@types/styled-components": "5.1.9",
    "@typescript-eslint/eslint-plugin": "4.18.0",
    "@typescript-eslint/parser": "4.18.0",
    "babel-loader": "8.2.2",
    "css-loader": "5.1.3",
    "eslint": "7.22.0",
    "eslint-config-typescript-shareable": "0.37.0",
    "eslint-import-resolver-node": "0.3.4",
    "eslint-plugin-functional": "3.2.1",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-react": "7.22.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "file-loader": "6.2.0",
    "html-webpack-plugin": "5.3.1",
    "pepjs": "0.5.3",
    "prettier": "2.2.1",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-is": "17.0.1",
    "semver": "7.3.4",
    "style-loader": "2.0.0",
    "styled-components": "5.2.1",
    "typescript": "4.2.3",
    "webpack": "5.27.0",
    "webpack-cli": "4.5.0",
    "webpack-dev-server": "3.11.2"
  }
}
